        <name>BAP_DelLogTable</name>
        <fields>id,tablename,ts,DelID</fields>
        <where>( tablename like 'aa_%' or tablename  like 'EAP_Hold%' or tablename = 'eap_user'  or tablename = 'eap_usergrouprelation' ) </where>
        <synctype>1</synctype>
