<?xml version="1.0" encoding="gb2312"?>
<syncparams>
 <tables>
    <table LoadFrom="Ufida.T.EAP.Sync.UA.dll" QueryClassName="Ufida.T.EAP.Sync.UA.InventoryQuery" >
      <name>aa_inventory</name>
      <!--id,code,name,specification,batchrunnumber,shorthand,productinfo,idinventoryclass,idunit,imageFile,Userfreeitem0,Userfreeitem1,Userfreeitem2,Userfreeitem3,Userfreeitem4,Userfreeitem5,Userfreeitem6,Userfreeitem7,Userfreeitem8,Userfreeitem9,ControlRangeFreeitem0,ControlRangeFreeitem1,ControlRangeFreeitem2,ControlRangeFreeitem3,ControlRangeFreeitem4,ControlRangeFreeitem5,ControlRangeFreeitem6,ControlRangeFreeitem7,ControlRangeFreeitem8,ControlRangeFreeitem9,priuserdefnvc1,priuserdefnvc2,priuserdefnvc3,priuserdefnvc4,priuserdefnvc5,priuserdefdecm1,priuserdefdecm2,priuserdefdecm3,priuserdefdecm4,priuserdefdecm5,ts,disabled,isnew-->
      <fields>id,code,name,specification,batchrunnumber,shorthand,productinfo,idinventoryclass,idunit,imageFile,Userfreeitem0,Userfreeitem1,Userfreeitem2,Userfreeitem3,Userfreeitem4,Userfreeitem5,Userfreeitem6,Userfreeitem7,Userfreeitem8,Userfreeitem9,ControlRangeFreeitem0,ControlRangeFreeitem1,ControlRangeFreeitem2,ControlRangeFreeitem3,ControlRangeFreeitem4,ControlRangeFreeitem5,ControlRangeFreeitem6,ControlRangeFreeitem7,ControlRangeFreeitem8,ControlRangeFreeitem9,priuserdefnvc1,priuserdefnvc2,priuserdefnvc3,priuserdefnvc4,priuserdefnvc5,priuserdefdecm1,priuserdefdecm2,priuserdefdecm3,priuserdefdecm4,priuserdefdecm5,ts,disabled,isnew,idUnitBySale, idunitgroup, isSingleUnit,idUnitByRetail,idbarcodesolution,IsQualityPeriod,IsBatch,IdMarketingOrgan,taxrate,MadeRecordDate,ProcureBatch,InvSCost,AvagCost,LatestCost,InventoryDescript,ValueType,LowQuantity,topQuantity,safeQuantity,PickBatch,StandardTurnoverDays,PickBatchMethod,UnitType,idwarehouse,idpartner,idUnitByPurchase,idUnitByStock,reNewGoodAheadDays,reNewGoodSellDays,isWeigh,IsSuite</fields>
      <where> (islaborcost = 1 or isSale = 1 or IsSuite = 1 ) </where>
      <!--同步类型：0 定时同步；1 打开终端同步；2 初始化时同步；3 只有打开时才同步 ；-->
      <synctype>0</synctype>
    </table>
