<?xml version="1.0" encoding="gb2312"?>
<config> 
  <!-- 中心服务器监听端口 -->
  <port>89</port>

  <!-- 会话超时时间-->
  <sessionTime>1800000</sessionTime>

  <!--中心会话存储目录 -->
  <sessionPath>clustersession</sessionPath>
  


  <!-- 负载均衡设置 -->
  <!-- 多播地址 224.0.0.0-239.255.255.255 -->
  <muticastIP>224.233.233.119</muticastIP>

  <!-- 多播地址 224.0.0.0-239.255.255.255 -->
  <muticastPort>86</muticastPort>
  <!-- 负载均衡控制  1:cluster 0:single Server -->
  <cluster>0</cluster>

  <!-- 负载均衡服务器列表, the Server list -->
  <clusters>localhost:83,localhost:84,localhost:85</clusters>
</config>

